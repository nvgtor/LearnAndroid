        <activity android:name=".view.threeD.rotate3dpic.Rotate3dActivity"/>
        <activity android:name=".view.threeD.threeDSliding.ThreeDSlidingActivity"/>
        <activity android:name=".view.viewPagerGallery.ViewPagerGalleryActivity"/>
